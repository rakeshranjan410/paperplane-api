{
  "name": "markdown-qa-api",
  "version": "1.0.0",
  "description": "Backend API for Markdown Q&A Processor - handles S3 and MongoDB operations",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:dev": "pm2 start ecosystem.config.js --env development",
    "pm2:stop": "pm2 stop paperplane-api",
    "pm2:restart": "pm2 restart paperplane-api",
    "pm2:reload": "pm2 reload paperplane-api",
    "pm2:delete": "pm2 delete paperplane-api",
    "pm2:logs": "pm2 logs paperplane-api",
    "pm2:monit": "pm2 monit",
    "pm2:status": "pm2 status"
  },
  "keywords": [
    "api",
    "express",
    "mongodb",
    "s3",
    "questions"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.478.0",
    "@aws-sdk/client-secrets-manager": "^3.914.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "mongodb": "^6.3.0",
    "morgan": "^1.10.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
